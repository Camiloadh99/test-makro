{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/src/(.*)",
            "dest": "index.ts"
        },
        {
            "src": "/",
            "dest": "frontend/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "frontend/$1"
        }
    ]
}