{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "src/frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/src/(.*)",
            "dest": "src/index.ts"
        },
        {
            "src": "/",
            "dest": "src/frontend/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "src/frontend/$1"
        }
    ]
}